  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "RegisterDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@example.com"
          },
          "username": {
            "type": "string",
            "example": "user123"
          },
          "password": {
            "type": "string",
            "example": "P@ssw0rd123"
          },
          "firstName": {
            "type": "string",
            "example": "Ada"
          },
          "lastName": {
            "type": "string",
            "example": "Lovelace"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@example.com"
          },
          "password": {
            "type": "string",
            "example": "P@ssw0rd123"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "RefreshDto": {
        "type": "object",
        "properties": {
          "refresh_token": {
            "type": "string",
            "example": "refresh_token"
          }
        },
        "required": [
          "refresh_token"
        ]
      },
      "UpdateUserDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "example": "user123"
          },
          "firstName": {
            "type": "string",
            "example": "Ada"
          },
          "lastName": {
            "type": "string",
            "example": "Lovelace"
          },
          "avatar": {
            "type": "string",
            "example": "https://example.com/avatar.png"
          },
          "preferences": {
            "type": "object",
            "example": {
              "lastProjectId": "project_123"
            }
          }
        }
      },
      "ChangePasswordDto": {
        "type": "object",
        "properties": {
          "currentPassword": {
            "type": "string",
            "example": "OldP@ssw0rd1"
          },
          "newPassword": {
            "type": "string",
            "example": "NewP@ssw0rd1"
          }
        },
        "required": [
          "currentPassword",
          "newPassword"
        ]
      },
      "ProjectViewportDto": {
        "type": "object",
        "properties": {
          "x": {
            "type": "number",
            "example": 0
          },
          "y": {
            "type": "number",
            "example": 0
          },
          "zoom": {
            "type": "number",
            "example": 1
          }
        },
        "required": [
          "x",
          "y",
          "zoom"
        ]
      },
      "QuickActionDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "qa-1"
          },
          "label": {
            "type": "string",
            "example": "Concis"
          },
          "instruction": {
            "type": "string",
            "example": "Rephrase more concisely."
          },
          "order": {
            "type": "number",
            "example": 0
          }
        },
        "required": [
          "id",
          "label",
          "instruction",
          "order"
        ]
      },
      "CreateProjectDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Untitled project"
          },
          "description": {
            "type": "string",
            "example": "Project description"
          },
          "systemPrompt": {
            "type": "string",
            "example": "You are a helpful assistant."
          },
          "isPublic": {
            "type": "boolean",
            "example": false
          },
          "viewport": {
            "$ref": "#/components/schemas/ProjectViewportDto"
          },
          "quickActions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/QuickActionDto"
            }
          }
        },
        "required": [
          "name"
        ]
      },
      "UpdateProjectDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "New name"
          },
